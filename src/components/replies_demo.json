    \r\u003cbr\u003e    });\r\u003cbr\u003e    return pr;\r\u003cbr\u003e\r\u003cbr\u003e    \r\u003cbr\u003e    \r\u003cbr\u003e}\r\u003cbr\u003e\r\u003cbr\u003efunction proceedtopaymet(orderid){\r\u003cbr\u003e    return new Promise(function (resolve,reject){\r\u003cbr\u003e        if(orderid){\r\u003cbr\u003e            resolve({\r\u003cbr\u003e                paymentstatus:&quot;1&quot;,\r\u003cbr\u003e                paymentresult:&quot;payment succes fully completed&quot;\r\u003cbr\u003e            })\r\u003cbr\u003e        }else{\r\u003cbr\u003e\r\u003cbr\u003e            reject(new Error(&quot;transaction failed try again&quot;))\r\u003cbr\u003e        }\r\u003cbr\u003e    })\r\u003cbr\u003e    \r\u003cbr\u003e}\r\u003cbr\u003e\r\u003cbr\u003efunction showorderdsummary(paymentdetails){\r\u003cbr\u003e    return new Promise(function(resolve,reject){\r\u003cbr\u003e        if(paymentdetails.paymentstatus==&quot;1&quot;){\r\u003cbr\u003e            resolve({\r\u003cbr\u003e                item:&quot;shirt&quot;,\r\u003cbr\u003e                status:&quot;2&quot;,\r\u003cbr\u003e                orderditem:&quot; your orderd is sucecss fully completed&quot;\r\u003cbr\u003e            })\r\u003cbr\u003e        }else{\r\u003cbr\u003e            reject(new Error(&quot;you have no orders right now&quot;))\r\u003cbr\u003e        }\r\u003cbr\u003e    })\r\u003cbr\u003e}\r\u003cbr\u003e\r\u003cbr\u003efunction walletupdate(summary){\r\u003cbr\u003e    return new Promise(function(resolve,reject){\r\u003cbr\u003e        if(summary.status==&quot;2&quot;){\r\u003cbr\u003e            const bal=3000;\r\u003cbr\u003e            const walletbalance=bal;\r\u003cbr\u003e            resolve({\r\u003cbr\u003e                walletupdate:walletbalance,\r\u003cbr\u003e\r\u003cbr\u003e                lotofthanks:&quot;thaku for odering have a nice day&quot;\r\u003cbr\u003e\r\u003cbr\u003e            })\r\u003cbr\u003e        }else{\r\u003cbr\u003e            reject (new Error(&quot;sry something went wrong&quot;));\r\u003cbr\u003e        }\r\u003cbr\u003e    })\r\u003cbr\u003e}\r\u003cbr\u003e\r\u003cbr\u003e\r\u003cbr\u003efunction validatecart(){\r\u003cbr\u003e    return true;\r\u003cbr\u003e}",
            "textOriginal": "hw completed nice class lot of thanks brother\n\n\nconst cart=[\"shoe\",\"bag\",\"shirt\"];\r\n\r\nconst promises=createorder(cart);\r\n\r\npromises.then(function(orderid){\r\n    console.log(orderid);\r\n    return orderid;\r\n})\r\n.then(function(orderid){\r\n   return proceedtopaymet(orderid);\r\n    \r\n})\r\n.then(function(paymentdetails){\r\n    console.log(paymentdetails.paymentresult);\r\n    return paymentdetails;\r\n})\r\n.then(function(paymentdetails){\r\n    return showorderdsummary(paymentdetails)\r\n\r\n})\r\n.then(function(summary){\r\n    console.log(summary.item);\r\n    console.log(summary.orderditem);\r\n    return summary;\r\n})\r\n.then(function(summary){\r\n    return walletupdate(summary);\r\n})\r\n.then(function (wallet){\r\n    console.log(wallet.walletupdate)\r\n    console.log(wallet.lotofthanks);\r\n})\r\n.catch(function(err){\r\n    console.log(err);\r\n})\r\n\r\n\r\n function createorder(cart){\r\n    const pr=new Promise (function (resolve,reject){\r\nif(!validatecart(cart)){\r\n    const err=new Error(\"card is not validate\")\r\n        reject(err)\r\n    }\r\n    orderid=\"12345\";\r\n    if(orderid){\r\n        setTimeout(function(){\r\n            resolve(orderid);\r\n        },5000);\r\n\r\n       \r\n \r\n    }\r\n    \r\n    });\r\n    return pr;\r\n\r\n    \r\n    \r\n}\r\n\r\nfunction proceedtopaymet(orderid){\r\n    return new Promise(function (resolve,reject){\r\n        if(orderid){\r\n            resolve({\r\n                paymentstatus:\"1\",\r\n                paymentresult:\"payment succes fully completed\"\r\n            })\r\n        }else{\r\n\r\n            reject(new Error(\"transaction failed try again\"))\r\n        }\r\n    })\r\n    \r\n}\r\n\r\nfunction showorderdsummary(paymentdetails){\r\n    return new Promise(function(resolve,reject){\r\n        if(paymentdetails.paymentstatus==\"1\"){\r\n            resolve({\r\n                item:\"shirt\",\r\n                status:\"2\",\r\n                orderditem:\" your orderd is sucecss fully completed\"\r\n            })\r\n        }else{\r\n            reject(new Error(\"you have no orders right now\"))\r\n        }\r\n    })\r\n}\r\n\r\nfunction walletupdate(summary){\r\n    return new Promise(function(resolve,reject){\r\n        if(summary.status==\"2\"){\r\n            const bal=3000;\r\n            const walletbalance=bal;\r\n            resolve({\r\n                walletupdate:walletbalance,\r\n\r\n                lotofthanks:\"thaku for odering have a nice day\"\r\n\r\n            })\r\n        }else{\r\n            reject (new Error(\"sry something went wrong\"));\r\n        }\r\n    })\r\n}\r\n\r\n\r\nfunction validatecart(){\r\n    return true;\r\n}",
